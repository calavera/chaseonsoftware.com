    "serve": "gatsby serve",
    "develop-functions": "netlify-lambda serve ./src/_netlify-functions",
    "build-netlify-functions": "netlify-lambda build ./src/_netlify-functions"
  },
  "dependencies": {
    "core-js": "^2.5.7",
    "dotenv": "^6.1.0",
    "dropbox": "^4.0.12",
    "gatsby": "next",
    "gatsby-image": "next",
    "gatsby-plugin-canonical-urls": "next",
    "gatsby-plugin-catch-links": "next",
    "gatsby-plugin-feed": "next",
    "gatsby-plugin-google-analytics": "next",
    "gatsby-plugin-netlify": "next",
    "gatsby-plugin-react-helmet": "^3.0.0-beta.3",
    "gatsby-plugin-sass": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-plugin-typography": "next",
    "gatsby-remark-autolink-headers": "^1.4.19",
    "gatsby-remark-prismjs": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "isomorphic-fetch": "^2.2.1",
    "netlify-lambda": "^1.0.2",
    "node-sass": "^4.9.0",
