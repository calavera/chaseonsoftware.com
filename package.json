{
  "name": "chaseadams.io",
  "description": "chaseadams.io",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf public",
    "develop": "gatsby develop --host localhost --port 8000",
    "get-content": "node scripts/get_dropbox_contents.js",
    "flow": "flow",
    "build": "npm run get-content && npm run gatsby-build && npm run build-netlify-functions",
    "develop-init": "npm install && npm run develop-ln-content",
    "develop-ln-content": "ln -s $HOME/Dropbox/Apps/chaseadams.io $PWD/src/content && ln -s $HOME/Dropbox/Apps/chaseadams.io-drafts $PWD/src/drafts",
    "gatsby-build": "rm -rf .cache && gatsby build",
    "serve": "gatsby serve",
    "develop-functions": "netlify-lambda serve ./src/_netlify-functions",
    "build-netlify-functions": "netlify-lambda build ./src/_netlify-functions"
  },
  "dependencies": {
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.13",
    "@fortawesome/free-brands-svg-icons": "^5.7.0",
    "@fortawesome/free-solid-svg-icons": "^5.7.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@mdx-js/mdx": "^0.16.6",
    "@mdx-js/tag": "^0.16.6",
    "core-js": "^2.5.7",
    "dotenv": "^6.1.0",
    "dropbox": "^4.0.12",
    "emotion": "^9.2.12",
    "emotion-server": "^9.2.12",
    "emotion-theming": "^10.0.6",
    "gatsby": "next",
    "gatsby-image": "next",
    "gatsby-mdx": "^0.3.4",
    "gatsby-plugin-canonical-urls": "next",
    "gatsby-plugin-catch-links": "next",
    "gatsby-plugin-emotion": "^2.0.7",
    "gatsby-plugin-feed": "next",
    "gatsby-plugin-google-analytics": "next",
    "gatsby-plugin-netlify": "next",
    "gatsby-plugin-react-helmet": "^3.0.0-beta.3",
    "gatsby-plugin-sharp": "next",
    "gatsby-plugin-typography": "next",
    "gatsby-remark-autolink-headers": "^1.4.19",
    "gatsby-remark-prismjs": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "github-slugger": "^1.2.1",
    "isomorphic-fetch": "^2.2.1",
    "mdx-utils": "0.0.2",
    "netlify-lambda": "^1.0.2",
    "prism-react-renderer": "^0.1.5",
    "prismjs": "^1.15.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-emotion": "^9.2.12",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "slugify": "^1.3.4",
    "typography": "^0.16.17"
  },
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@gitlab.com:chaseadamsio/chaseadamsio.gitlab.io.git",
  "author": "Chase Adams <hey@chaseadams.io>",
  "devDependencies": {
    "flow-bin": "^0.87.0",
    "gatsby-plugin-flow": "^1.0.2"
  }
}
